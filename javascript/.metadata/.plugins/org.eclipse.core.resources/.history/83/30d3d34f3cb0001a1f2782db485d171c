function find(){
	
	var num = [];
	var i = 0;
	
	while(true){
		num[i]=Math.floor(Math.random()*45);
		i++;
		for(var k=0; k<6; k++){
			if(i!=k){
				if(num[i]==num[k]){
					i--;
				}
			}
		}
		if(i==6){
			break;
		}
	}
	
	document.write(sum.sort(),"<br />");
	
	
	


}
