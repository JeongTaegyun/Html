	<table border=1 align=center width=800 height=280>
		<tr>
			<script language="javascript">
			var num = 1;
			var t = "<table border='1'>";
			for(var i=1; i<=4; i++){
				t += "<tr>";
				for(var k=1; k<=3; k++){
					t += "<td>" + num + "</td>";
					num++;
				}
				t += "</tr>";
			}
			
			t += "</table>";
			console.log(t);
			document.write(t);
			</script>
		</tr>
	</table>
</body>
