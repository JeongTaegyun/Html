<script type="text/javascript">
var price1=3000;
var price2=3000;
var price3=3000;

var result1 = price1+price2+price3;
var result2 = result1 > 10000 ? "원 초과" : "돈 관리 잘 했어요!";
if(result1 > 10000){
	document.write((result1-10000) + result2);
}else{
	document.write(result2);
}



</script>
