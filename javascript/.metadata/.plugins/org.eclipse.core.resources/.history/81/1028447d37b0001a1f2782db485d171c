  <title> lotto </title>
  <script>

function find(){
	
	var sum = [1,2,3,4,5,6];
	
	
	for(var i=0; i<6; i++){
		sum[i]=Math.random()*45
	}
	
	document.write(sum.sort(),"<br />";)
	
	
	


}
  </script>
